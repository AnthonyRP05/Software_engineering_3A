          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mUfHk6vEEeuapJcBr8lIhA" name="getSongsRandomly" specification="_l5IoUKvEEeuapJcBr8lIhA">
            <ownedParameter xmi:id="_mUfHlKvEEeuapJcBr8lIhA" type="_mPy00avEEeuapJcBr8lIhA" direction="return"/>
            <language>java</language>
            <body>ArrayList&lt;UUID> shuffledSongs = songsUIDs;&#xD;
		Collections.shuffle(shuffledSongs);&#xD;
		return shuffledSongs;&#xD;
	</body>
