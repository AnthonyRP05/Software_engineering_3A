          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mUdSYKvEEeuapJcBr8lIhA" name="Album" specification="_l45-0KvEEeuapJcBr8lIhA">
            <ownedParameter xmi:id="_mUdSYavEEeuapJcBr8lIhA" name="title">
