              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            </ownedParameter>
            <language>java</language>
            <body>switch (language.toLowerCase()) {&#xD;
			case &quot;english&quot;:&#xD;
			default:&#xD;
				this.language = Language.ENGLISH;&#xD;
				break;&#xD;
			case &quot;french&quot;:&#xD;
				this.language = Language.FRENCH;&#xD;
				break;&#xD;
			case &quot;german&quot;:&#xD;
				this.language = Language.GERMAN;&#xD;
				break;&#xD;
			case &quot;spanish&quot;:&#xD;
				this.language = Language.SPANISH;&#xD;
				break;&#xD;
			case &quot;italian&quot;:&#xD;
				this.language = Language.ITALIAN;&#xD;
				break;&#xD;
				&#xD;
		}&#xD;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mUjZAKvEEeuapJcBr8lIhA" name="setCategory" specification="_l51L4KvEEeuapJcBr8lIhA">
