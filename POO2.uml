            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mVKdAKvEEeuapJcBr8lIhA" name="getAlbumsTitlesSortedByDate" specification="_l9b5gavEEeuapJcBr8lIhA">
              <ownedParameter xmi:id="_mVKdAavEEeuapJcBr8lIhA" direction="return">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <language>java</language>
              <body>StringBuffer titleList = new StringBuffer();&#xD;
		Collections.sort(albums, new SortByDate());&#xD;
		for (Album al : albums)&#xD;
			titleList.append(al.getTitle()+ &quot;\n&quot;);&#xD;
		return titleList.toString();&#xD;
	</body>
