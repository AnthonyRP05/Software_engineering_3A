          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mVBTEqvEEeuapJcBr8lIhA" name="printAvailableCommands" specification="_l7f_0avEEeuapJcBr8lIhA">
            <language>java</language>
            <body>System.out.println(&quot;b: update the database&quot;);&#xD;
		System.out.println(&quot;t: display the album titles, ordered by date&quot;);&#xD;
		System.out.println(&quot;g: display songs of an album, ordered by genre&quot;);&#xD;
		System.out.println(&quot;d: display songs of an album&quot;);&#xD;
		System.out.println(&quot;u: display audiobooks ordered by author&quot;);&#xD;
		System.out.println(&quot;p: play some musics&quot;);&#xD;
		System.out.println(&quot;k: pause / play the song&quot;);&#xD;
		System.out.println(&quot;q: quit program&quot;);&#xD;
	</body>
