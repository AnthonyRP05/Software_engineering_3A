            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_mS4lEKvEEeuapJcBr8lIhA" value="&quot;localhost&quot;"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_mS6aQKvEEeuapJcBr8lIhA" name="ss" visibility="private" type="_mQG94KvEEeuapJcBr8lIhA" association="_mVnwAKvEEeuapJcBr8lIhA"/>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mUoRgKvEEeuapJcBr8lIhA" name="connect" specification="_l6prQKvEEeuapJcBr8lIhA">
            <ownedParameter xmi:id="_mUoRgavEEeuapJcBr8lIhA" name="ip">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            </ownedParameter>
            <language>java</language>
            <body>try {&#xD;
			//the server socket is defined only by a port (its IP is localhost)&#xD;
			ss = new ServerSocket (6666);&#xD;
			System.out.println(&quot;Server waiting for connection...&quot;);&#xD;
			new ServerInterface().start();&#xD;
			while (true) {&#xD;
				Socket socket = ss.accept();//establishes connection&#xD;
				System.out.println(&quot;Connected as &quot; + ip);&#xD;
				// create a new thread to handle client socket&#xD;
				new ServerThread(socket).start();&#xD;
			}&#xD;
		} catch (IOException ioe) {&#xD;
			ioe.printStackTrace();&#xD;
			//if IOException close the server socket&#xD;
			if (ss != null &amp;&amp; !ss.isClosed()) {&#xD;
				try {&#xD;
					ss.close();&#xD;
				} catch (IOException e) {&#xD;
					e.printStackTrace(System.err);&#xD;
				}&#xD;
			}&#xD;
		}&#xD;
	</body>
          </ownedBehavior>
